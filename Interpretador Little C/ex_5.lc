/*
    demonstra uso de argumentos de função
*/
main(){
    f2(10,f1(10,20),99);
}
f1(int a,int b){
    int contador;
    print("em f1");
    puts("");
    contador =a;
    do {
        print(contador);
        puts("");
    }while(contador=contador-1);
    print(a);
    print(b);
    print(a*b);
    return a*b;
}
f2(int a,int x, int y){
    print("em f2");
    puts("");
    print(a);print(x);
    print(x/a);
    print(y*x);
}
